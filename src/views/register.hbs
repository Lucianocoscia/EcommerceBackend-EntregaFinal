 <div class="container my-5">
      <div class="jumbotron">
        <h1>SIGNUP</h1>
        <br />
        <form
          class="form"
          id="signup-form"
          role="form"
          action="/register"
          method="post"
          autocomplete="off"
           enctype="multipart/form-data"
        >
          <div class="form-group">
            <input
              name="username"
              placeholder="Nombre"
              class="form-control my-3"
              type="text"
              required
            />
            <input
              name="password"
              id="password"
              placeholder="Contrasenia"
              class="form-control my-3"
              type="password"
              required
            />
            <input
              name="checkPassword" 
              id="checkPassword" 
              placeholder="Repetir contraseña"
              class="form-control my-3"
              type="password"
              required
            />
            <input
              name="firstname"
              placeholder="FirstName"
              class="form-control my-3"
              type="text"
              required
            />
            <input
              name="lastname"
              placeholder="LastName"
              class="form-control my-3"
              type="text"
              required
            />

            <input
              name="email"
              placeholder="Email"
              class="form-control my-3"
              type="text"
              required
            />
            <input
              name="phone"
              placeholder="Phone"
              class="form-control my-3"
              type="tel"
              required
            />
            <input
              name="address"
              placeholder="Address"
              class="form-control my-3"
              type="addres"
              required
            />
            <input
              name="age"
              placeholder="Age"
              class="form-control my-3"
              type="number"
              required
            />
            <input name="photo" class="form-control" type="file" required />
          </div>
          <div class="form-group">
            <input class="btn btn-success my-3" type="submit" value="Singup" />
          </div>
        </form>
      </div>
    </div>
            <div class="text-center"></div>
        <button class="btn btn-success my-3" onclick="goSignup()">
          LOGIN
        </button>
      </div>
    </div>

    <script>
      // Check if password is the same
      const form = document.getElementById('signup-form');
        const password = document.getElementById("password")
        const checkPassword = document.getElementById("checkPassword")
        form.addEventListener('submit', (event) => {
            if (password.value !== checkPassword.value) {
                event.preventDefault();
                alert('Las contraseñas no coinciden. Por favor, verifique los campos de contraseña.');
            }
        });

      function redireccionar(pagina) {
        location.href = pagina;
      }

      function goSignup() {
        redireccionar("/");
      }
    </script>